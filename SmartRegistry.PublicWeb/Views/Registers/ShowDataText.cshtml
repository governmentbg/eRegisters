@using SmartRegistry.Domain.Common
@using System.Collections.Generic;
@using SmartRegistry.PublicWeb.Controllers
@using SmartRegistry.Domain.Entities
@using SmartRegistry.Domain.Services
@model List<TempData>
@{
    ViewBag.Title = "Информация за запис";
}
 <div class="container">
        <div class="main-content">
            <div class="row">
                <div class="col-12">
                    <div class="page-title-box d-flex align-items-center justify-content-between">
                        <div class="breadcrumb">
                            <a href="/">Начало</a> / @ViewBag.GroupName
                        </div>
                    </div>
                </div>
            
            </div>

            <div class="page-content">

<h1>ShowDataText</h1>

@if (Model.Count > 0)
{


    foreach (var tempdata in Model)
    {
        <div class="row-content">
             
        <h3> @tempdata.Title</h3>
        <p> @tempdata.Value</p>
        </div>
    }

   

}


            </div>
            </div>

    </div>
